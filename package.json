{
    "name": "razor-calendar",
    "private": false,
    "version": "0.3.1",
    "type": "module",
    "author": {
        "email": "feras.ahmad@gmx.de",
        "name": "Feras Ahmad"
    },
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "files": [
        "/dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Eng-FerasAhmad/razor-calendar.git"
    },
    "keywords": [
        "react",
        "calendar",
        "vite",
        "razor",
        "storybook"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Eng-FerasAhmad/razor-calendar/issues"
    },
    "homepage": "https://github.com/Eng-FerasAhmad/razor-calendar/blob/master/README.md",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "eslint": "eslint \"./src/**/*.{ts,tsx}\"",
        "eslint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "npx prettier --write . ",
        "stylelint": "stylelint \"./src/**/styles.ts\"",
        "stylelint:fix": "stylelint \"./src/**/styles.ts\" --fix",
        "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix && npx prettier --write . && stylelint \"./src/**/styles.ts\" --fix",
        "preview": "vite preview",
        "test": "cross-env NODE_ENV=test jest --silent",
        "test:verbose": "cross-env NODE_ENV=test jest",
        "test:coverage": "cross-env NODE_ENV=test npm run test"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/modifiers": "^9.0.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/material": "^6.3.1",
        "@mui/x-date-pickers": "^7.23.3",
        "i18next": "^23.16.5",
        "i18next-browser-languagedetector": "^8.0.0",
        "luxon": "^3.5.0",
        "razor-icon-library": "^0.2.17",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.0.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.5.1",
        "@types/jest": "^29.5.11",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20.10.6",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "@vitejs/plugin-react": "^4.1.0",
        "cross-env": "^7.0.3",
        "cross-fetch": "^4.0.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-import-resolver-vite": "^1.3.2",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-unused-imports": "^3.2.0",
        "flush-promises": "^1.0.2",
        "jest": "^29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^29.7.0",
        "postcss-styled-syntax": "^0.6.4",
        "prettier": "^3.0.3",
        "stylelint": "^16.5.0",
        "stylelint-config-recommended": "^14.0.0",
        "stylelint-config-standard": "^36.0.0",
        "ts-jest": "^29.1.4",
        "typescript": "^5.2.2",
        "vite": "^6.3.2",
        "vite-plugin-dts": "^4.2.3",
        "vite-tsconfig-paths": "^4.3.1"
    }
}
